  "scripts": {
    "mock": "node ./mock-server/index.js",
    "dev-server": "node build/dev-server.js",
    "dev": "npm-run-all --parallel mock dev-server",
    "build": "node build/build.js",
    "unit": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
    "e2e": "node test/e2e/runner.js",
    "test": "npm run unit && npm run e2e",
    "lint": "eslint --ext .js,.vue src test/unit/specs test/e2e/specs",
    "prod":"npm-run-all --parallel mock server",
    "server": "http-server dist/",
    "scss": "node-sass --output-style compressed src/resources/assets/sass/themes -o static/css/themes",
    "scssenso": "node-sass --output-style compressed src/resources/assets/sass/enso.scss static/css/enso.css"
